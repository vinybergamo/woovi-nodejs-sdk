{
  "name": "@woovi/node-sdk",
  "version": "1.0.1",
  "description": "Woovi NodeJS SDK",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "git@github.com:woovibr/woovi-nodejs-sdk.git",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@swc/core": "^1.11.24",
    "@swc/jest": "^0.2.38",
    "@types/jest": "^29.5.14",
    "commitizen": "^4.3.1",
    "jest": "^29.7.0",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "commit": "cz",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "lint": "biome lint ./src ./tests",
    "format": "biome check ./src ./tests --apply",
    "prepare": "pnpm build"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "pre-commit": {
    "run": "lint"
  },
  "dependencies": {
    "@woovi/apiwithlog": "^2.6.0"
  }
}
